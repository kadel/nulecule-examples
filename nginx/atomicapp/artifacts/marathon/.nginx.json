{
    "id": "nginx",
    "container": {
        "type": "DOCKER",
        "docker": {
            "network": "BRIDGE",
            "image": "tomaskral/nonroot-nginx",
            "portMappings": [
            {
                "containerPort": 8080,
                "servicePort": 80,
                "hostPort": 0,
                "protocol": "tcp"
            }
            ]
        }
    },
    "cpus": 0.5,
    "mem": 64
}

